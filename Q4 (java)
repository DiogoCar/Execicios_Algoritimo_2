    public static void main(String[] args) {
        // TODO code application logic here

    
    double p,h,h2,imc;
        
        String peso,alt;
   
        
        peso =JOptionPane.showInputDialog(null, "Digite o peso: ");
        alt =JOptionPane.showInputDialog(null, "Digite a altura: ");
         
        p = Double.parseDouble(peso);
        h = Double.parseDouble(alt);
        
          h2= h*h;
          imc= p*h2;
          
      JOptionPane.showMessageDialog(null, "Seu IMC Ã©: " + imc);
       
    }
}
       
