public class Exercicio_fix {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here

    
    double sal,por,por2,salf;
        
        String salario;
   
        
        salario =JOptionPane.showInputDialog(null, "Digite o salario: ");
    
        
        sal = Double.parseDouble(salario);
        
        
       por = 14 * sal;
       por2 = por/100;
       salf = sal + por2;
       
      JOptionPane.showMessageDialog(null, "Seu aumento com salario familia Ã©: " + salf);
       
    }
}
