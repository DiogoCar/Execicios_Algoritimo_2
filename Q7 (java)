public class Exercicio_fix {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here

    
    double a1,a2,a3,b1,b2,b3,c1,c2,c3;
        
        String sa1,sa2,sa3,sb1,sb2,sb3;
   
        
        sa1 =JOptionPane.showInputDialog(null, "Digite o valor: ");
        sa2 =JOptionPane.showInputDialog(null, "Digite o valor: ");
        sa3 =JOptionPane.showInputDialog(null, "Digite o valor: "); 
        sb1 =JOptionPane.showInputDialog(null, "Digite o valor: ");
        sb2 =JOptionPane.showInputDialog(null, "Digite o valor: ");
        sb3 =JOptionPane.showInputDialog(null, "Digite o valor: "); 
    
 
        a1 = Double.parseDouble(sa1);
        a2 = Double.parseDouble(sa2);
        a3 = Double.parseDouble(sa3);
        b1 = Double.parseDouble(sb1);
        b2 = Double.parseDouble(sb2);
        b3 = Double.parseDouble(sb3);
        
        c1 = a1 + b1;        
        c2 = a2 + b2;
        c3 = a3 + b3;
        
        

        
      JOptionPane.showMessageDialog(null, "resultado: " + c1 + c2 + c3);
       
    }
}
