package exercicio_fix;

import javax.swing.JOptionPane;

/**
 *
 * @author Discente
 */
public class Exercicio_fix {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here

    
    double n1,n2,dif;
        
        String nm1,nm2;
   
        
        nm1 =JOptionPane.showInputDialog(null, "Digite o valor: ");
        nm2 =JOptionPane.showInputDialog(null, "Digite o valor: ");
        
    
 
        n1 = Double.parseDouble(nm1);
        n2 = Double.parseDouble(nm2);
      
        
       dif = n1 - n2;
        
        

        
      JOptionPane.showMessageDialog(null, "resultado: " + dif);
       
    }
}
